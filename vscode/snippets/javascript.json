			"import { createFolder } from '../../../helpers/helperFile.js';",
			"",
			"",
			"export const generate${1} = async(",
			"  fullPath, ",
			"  namespace,",
			"  singularName,",
			"  pluralName,",
			"  singularNameKebab,",
			"  pluralNameKebab,",
			"  singularNameSnake,",
			"  pluralNameSnake,",
			"  singularNameCamel,",
			"  pluralNameCamel,",
			"  columns",
			") => {    ",
			"",
			"    // Folder",
			"    const folderPath = path.join(fullPath, 'src', '${2}');",
			"    ",
			"    // File",
			"    const filePath = path.join(folderPath, '${3}');",
			"",
			"    // Asegurar que la carpeta exista",
			"    createFolder(folderPath);",
			"",
			"",
			"    // Code",
			"    const code = `",
			"    ${4}",
			"`.trimStart();",
			"",
			"  try {",
			"    fs.writeFileSync(filePath, code);",
			"    console.log(`✅ Archivo creado: \\${filePath}`.green);",
			"  } catch (error) {",
			"    console.error(`❌ Error al crear archivo: ${error.message}`);",
			"  }",
			"",
			"}",
		],
