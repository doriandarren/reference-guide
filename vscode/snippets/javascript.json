	"Generator Main Snippet": {
		"prefix": "ngmain",
		"body": [
			"import { clearScreen, menuMain, pause } from \"../../helpers/inquirer.js\";",
			"",
			" // Snippet: 1) Minúscula 2) Mayúcula",
			"export const ${1}Main = async () => {",
			"  let opt = \"\";",
			"",
			"  clearScreen();",
			"",
			"",
			"  do {",
			"    opt = await menuMain([",
			"      { name: \"Proyecto\", value: \"proyect\" },",
			"      { name: \"Módulo - CRUD\", value: \"module_crud\" },",
			"      { name: \"Módulo - SINGLE\", value: \"module_single\" },",
			"      { name: \"Módulo - Eliminar\", value: \"module_delete\" },",
			"      { name: \"Atrás\", value: \"back\" },",
			"    ], '${2}');",
			"",
			"    switch (opt) {",
			"      case \"proyect\":",
			"        await startProject${2}();",
			"        break;",
			"",
			"      case \"module_crud\":",
			"        await startModuleCrud${2}();",
			"        break;",
			"      ",
			"      case \"module_single\":",
			"        await startModuleSingle${2}();",
			"        break;",
			"      ",
			"      case \"module_delete\":",
			"        await startModuleDelete${2}();",
			"        break;",
			"",
			"      default:",
			"        break;",
			"    }",
			"",
			"    await pause();",
			"    ",
			"  } while (opt != \"back\");",
			"};"
		],
		"description": "Snippet para generar el menú principal"
	},
