	"Node Create Router": {
		"prefix": "nr",
		"body": [
			"import { Router } from \"express\";",
			"import { check } from \"express-validator\";",
			"import { validateFields } from \"../middlewares/validate-fields.js\";",
			"",
			"const router = Router();",
			"",
			"/**",
			" * List",
			" */",
			"router.get('/', [], ${1:template}ListController);",
			"",
			"/**",
			" * Show",
			" */",
			"router.get('/:id', [",
			"    check('name', 'El name es obligatorio').not().isEmpty(),",
			"    validateFields",
			"], ${1:template}ShowController);",
			"",
			"/**",
			" * Store",
			" */",
			"router.post('/', [], ${1:template}StoreController);",
			"",
			"/**",
			" * Update",
			" */",
			"router.put('/:id', [], ${1:template}UpdateController);",
			"",
			"/**",
			" * Delete",
			" */ ",
			"router.delete('/:id', [], ${1:template}DeleteController);",
			"",
			"",
			"export default router;",
		],
		"description": "Create Node Router"
	},
